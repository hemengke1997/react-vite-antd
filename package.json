{
  "name": "views",
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "commitlint": "commitlint --from=master",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --cache --ext .js,.jsx,.ts,.tsx --no-error-on-unmatched-pattern --quiet --fix ./src",
    "lint:style": "stylelint --cache --fix \"**/*.{less,postcss,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "postinstall": "husky install",
    "upgrade:deps": "yarn upgrade-interactive --latest"
  },
  "lint-staged": {
    "**/*.less": "npm run lint:style",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@loadable/component": "^5.15.0",
    "@types/warning": "^3.0.0",
    "antd": "4.17.0-alpha.9",
    "classnames": "^2.3.1",
    "context-state": "^1.1.0",
    "dayjs": "^1.10.7",
    "eslint-plugin-react": "^7.27.0",
    "lodash": "^4.17.21",
    "memoize-one": "^6.0.0",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "5.2.1",
    "react-router-dom": "5.3.0",
    "react-transition-group": "^4.4.2",
    "react-use": "^17.3.1",
    "recoil": "^0.5.2",
    "swr": "^1.0.1",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@types/loadable__component": "^5.13.4",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.7",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@vitejs/plugin-legacy": "^1.6.2",
    "@vitejs/plugin-react": "^1.0.9",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "esno": "^0.12.0",
    "husky": "^7.0.4",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^12.0.2",
    "postcss": "^8.3.11",
    "postcss-less": "^5.0.0",
    "prettier": "^2.4.1",
    "rollup-plugin-visualizer": "^5.5.2",
    "stylelint": "^14.1.0",
    "stylelint-config-css-modules": "^2.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^6.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.6.14",
    "vite-plugin-style-import": "^1.3.0",
    "vite-plugin-windicss": "^1.5.1",
    "windicss": "^3.2.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
