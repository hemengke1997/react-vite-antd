{
  "name": "views",
  "version": "0.0.0",
  "scripts": {
    "start": "vite --mode development",
    "build": "cross-env NODE_ENV=production vite build --mode production",
    "serve": "vite preview --mode development",
    "report": "cross-env REPORT=true npm run build",
    "commitlint": "commitlint --from=master",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --cache --ext .js,.jsx,.ts,.tsx --no-error-on-unmatched-pattern --quiet --fix ./src",
    "lint:style": "stylelint --cache --fix \"**/*.{less,postcss,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "postinstall": "husky install",
    "upgrade:deps": "yarn upgrade-interactive --latest",
    "prepush": "esno ./scripts/prepush.ts"
  },
  "lint-staged": {
    "**/*.less": "npm run lint:style",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ahooksjs/use-url-state": "^3.0.0-alpha.4",
    "@ant-design/icons": "^4.7.0",
    "@loadable/component": "^5.15.0",
    "@tanem/react-nprogress": "^3.0.82",
    "ahooks": "^3.0.0-alpha.7",
    "antd": "4.17.0",
    "axios": "^0.24.0",
    "blueimp-md5": "^2.19.0",
    "classnames": "^2.3.1",
    "context-state": "^1.1.0",
    "dayjs": "^1.10.7",
    "lodash-es": "^4.17.21",
    "memoize-one": "^6.0.0",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.2.0",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-resizable": "^3.0.4",
    "react-router": "5.2.1",
    "react-router-dom": "5.3.0",
    "react-transition-group": "^4.4.2",
    "react-use": "^17.3.1",
    "recoil": "^0.5.2",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/inquirer": "^8.1.3",
    "@types/loadable__component": "^5.13.4",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^16.11.9",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.36",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/semver": "^7.3.9",
    "@types/shelljs": "^0.8.9",
    "@types/warning": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-legacy": "^1.6.2",
    "@vitejs/plugin-react": "^1.0.9",
    "autoprefixer": "^10.4.0",
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.1.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "esno": "^0.12.1",
    "husky": "^7.0.4",
    "inquirer": "^8.2.0",
    "less": "^4.1.2",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^12.1.1",
    "postcss": "^8.3.11",
    "postcss-less": "^5.0.0",
    "prettier": "^2.4.1",
    "rollup-plugin-visualizer": "^5.5.2",
    "semver": "^7.3.5",
    "shelljs": "^0.8.4",
    "stylelint": "^14.1.0",
    "stylelint-config-css-modules": "^2.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^6.0.0",
    "typescript": "^4.5.2",
    "vite": "^2.6.14",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-style-import": "^1.3.0",
    "vite-plugin-windicss": "^1.5.1",
    "windicss": "^3.2.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
