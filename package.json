{
  "name": "views",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "commitlint": "commitlint --from=master",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --cache --ext .js,.jsx,.ts,.tsx --no-error-on-unmatched-pattern --quiet --fix ./src",
    "lint:style": "stylelint --cache --fix \"**/*.{less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "postinstall": "husky install",
    "upgrade:deps": "yarn upgrade-interactive --latest"
  },
  "lint-staged": {
    "**/*.less": "npm run lint:style",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "dayjs": "^1.10.7",
    "eslint-plugin-react": "^7.26.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vitejs/plugin-legacy": "^1.6.2",
    "@vitejs/plugin-react": "^1.0.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^11.2.6",
    "postcss": "^8.3.11",
    "postcss-less": "^5.0.0",
    "prettier": "^2.4.1",
    "rollup-plugin-visualizer": "^5.5.2",
    "stylelint": "^14.0.1",
    "stylelint-config-css-modules": "^2.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^23.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.6.4",
    "vite-plugin-windicss": "^1.5.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
